     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 330
#define PI 3.14159265358979
#define MAX_PARAMS 1

uniform mat4 uWorldViewProj;
uniform mat4 uWorldView;
uniform float uTime;
uniform vec3 uLightDir;
uniform vec3 uEyePosition;
uniform float[MAX_PARAMS] uA;
uniform float[MAX_PARAMS] uL;
uniform float[MAX_PARAMS] uS;
uniform vec3[MAX_PARAMS] uDir;
uniform float uQ;

in vec3 rm_Vertex;
in vec3 rm_Normal;

out vec3 Normal;
out vec3 Binormal;
out vec3 Tangent;
out vec3 LightDir;
out vec3 ViewDir;
out vec3 Texcoord;
out float Time;
const float g = 9.8;

void main(void)
{
   Time = uTime;
   float W[MAX_PARAMS];
   float phi[MAX_PARAMS];
   float Q[MAX_PARAMS];
   vec3 Dir[MAX_PARAMS];
   vec3 TVertex = rm_Vertex;
   float z = 0;
   float x = 0;
   float y = 0;
   Texcoord = vec3(0,0,0);
   
   for (int i = 0; i < MAX_PARAMS; i++)
   {
      float dx, dy;
      if (uL[i] != 0)
      {
         W[i] = sqrt(g*2*PI/uL[i]);
         phi[i] = uS[i] * W[i];
         Q[i] = uQ/(uA[i]*W[i]*MAX_PARAMS);
         Dir[i] = normalize(uDir[i]);
      }      
      
      z += uA[i] * sin(W[i] * dot(Dir[i], vec3(TVertex.x, TVertex.y, 0)) + uTime*phi[i]);
      x += dx = Q[i]*uA[i]*dot(Dir[i], vec3(TVertex.x, 0, 0))*cos(W[i] * dot(Dir[i], vec3(TVertex.x, TVertex.y, 0.0)) + uTime*phi[i]);
      y += dy = Q[i]*uA[i]*dot(Dir[i], vec3(0, TVertex.y, 0))*cos(W[i] * dot(Dir[i], vec3(TVertex.x, TVertex.y, 0.0)) + uTime*phi[i]);      
      
      Texcoord.x += TVertex.x + dx * Dir[i].x;      
      Texcoord.y += TVertex.y + dy * Dir[i].y;      
   }
  
   vec3 dBinormal = vec3(0,0,0);
   vec3 dNormal = vec3(0,0,0);
   vec3 dTangent = vec3(0,0,0);
   vec3 Vertex = vec3(rm_Vertex.x + x, rm_Vertex.y + y, z); 
   
   for (int i = 0; i < MAX_PARAMS; i++)
   {       
      float WA = W[i]*uA[i];
      float SC = sin(W[i]*dot(Dir[i], Vertex)+phi[i]*uTime);
      float CC = cos(W[i]*dot(Dir[i], Vertex)+phi[i]*uTime);
      
      dBinormal.x += Q[i]*dot(Dir[i], vec3(Vertex.x*Vertex.x, 0, 0))*WA*SC;        
      dBinormal.y += Q[i]*dot(Dir[i], vec3(Vertex.x,0,0))*dot(Dir[i], vec3(0, Vertex.y, 0)) * WA * SC;
      dBinormal.z += dot(Dir[i], vec3(Vertex.x,0,0))*WA*SC;
      
      dTangent.x += Q[i]*dot(Dir[i], vec3(Vertex.x,0,0))*dot(Dir[i], vec3(0, Vertex.y, 0)) * WA * SC;
      dTangent.z += Q[i]*dot(Dir[i], vec3(0, Vertex.y*Vertex.y, 0)) * WA * SC;
      dTangent.z += dot(Dir[i], vec3(0, Vertex.y, 0)) * WA * CC;
      
      dNormal.x += dot(Dir[i], vec3(Vertex.x, 0, 0))*WA*CC;
      dNormal.y += dot(Dir[i], vec3(0, Vertex.y, 0))*WA*CC;
      dNormal.z += Q[i]*WA*SC;
   }
   
   vec3 TBinormal, TNormal, TTangent;
   TBinormal.x = 1 - dBinormal.x;
   TBinormal.y = -dBinormal.y;
   TBinormal.z = dBinormal.z;
   
   TTangent.x = -dTangent.x;
   TTangent.y = 1 - dTangent.y;
   TTangent.z = dTangent.z;
   
   TNormal.x = -dNormal.x;
   TNormal.y = -dNormal.y;
   TNormal.z = 1 - dNormal.z; 
   
   
   gl_Position = uWorldViewProj * vec4(Vertex, 1.0);
    
   mat3 NormalMatrix = transpose(inverse(mat3(uWorldView[0].xyz, uWorldView[1].xyz, uWorldView[2].xyz)));
   
   Normal = normalize(NormalMatrix * TNormal);     
   Tangent = normalize(NormalMatrix * TTangent);
   Binormal = normalize(NormalMatrix * TBinormal); 
   
      
   vec3 LightDirection = vec3(uWorldView * vec4(uLightDir, 1.0));
   vec4 ObjectPosition = uWorldView * vec4(Vertex, 1.0);
   
   vec3 ViewDirection = -vec3(uWorldView * vec4(Vertex,1.0));// - ObjectPosition.xyz;
   
   LightDir.x = dot(Tangent, LightDirection);
   LightDir.y = dot(Binormal, LightDirection);
   LightDir.z = dot(Normal, LightDirection);
    
   ViewDir.x = dot(Tangent, ViewDirection);
   ViewDir.y = dot(Binormal, ViewDirection);
   ViewDir.z = dot(Normal, ViewDirection);

   //ViewDir = ViewDirection;
   Texcoord = Vertex;        
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 330

uniform sampler2D uBumpMap;
uniform sampler2D uSkyMap;
uniform samplerCube uCubeMap;
in vec3 Normal;
in vec3 Binormal;
in vec3 Tangent;
in vec3 LightDir;
in vec3 ViewDir;
in vec3 Texcoord;
in float Time;
out vec4 FragColor;
out float FragDepth;

void main(void)
{
   vec2 dnoise = noise2(Texcoord + Time);
   vec2 TCoord = Texcoord.xy + dnoise;
   vec3 dnoise3 = noise3(Texcoord+Time);
   vec3  fvLightDirection = normalize( LightDir );
   vec3  fvNormal         = normalize(( texture( uBumpMap, TCoord ).xyz * 2.0 ) - 1.0 );
   float fNDotL           = dot( fvNormal, fvLightDirection ); 
   
   vec3  fvReflection     = normalize( ( ( 2.0 * fvNormal ) * fNDotL ) - fvLightDirection ); 
   vec3  fvViewDirection  = normalize( ViewDir );
   float fRDotV           = max( 0.0, dot( fvReflection, fvViewDirection ) );
   
   vec3 fvViewRef = reflect(-fvViewDirection, fvNormal);
   vec4  fvBaseColor      = texture( uCubeMap, fvViewRef);
   
      
   vec4  fvTotalDiffuse   = fvBaseColor; 
   vec4  fvTotalSpecular  = vec4(1,1,1,1) * ( pow( fRDotV, 125 ) );
  
   FragColor = (fvTotalDiffuse + fvTotalSpecular);//vec4(ViewDir,1);//( fvBaseColor+fvTotalSpecular );   
   FragDepth = 1;
}]]></RmShaderCode></RmGLShader>
