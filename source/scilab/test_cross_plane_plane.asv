p3 = [0.00384503; 0.92312; -0.384492; 0];
p4 = [0.058291; 0; -0.9983; -0.966106];
p5 = [0; -0.926518; -0.376251; 0.0752486];
p6 = [0; 0.926518; -0.376251; 0.0752486];
p7 = [-0.575987; 0; 0.817459; 11.3557];

clip_space = {p0 p1 p2 p3 p4 p5 p6 p7};

for i = 1 : length(clip_space) - 1
    for j = i+1 : length(clip_space)
