        disp([num2str(i-1), '<->', num2str(j-1)]);
        disp(['Cross: (', num2str(clip_space{i}(1)), ', ', ...
num2str(clip_space{i}(2)), ', ', ...
            num2str(clip_space{i}(3)), ...
